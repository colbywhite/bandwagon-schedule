{
  "private": true,
  "scripts": {
    "start": "rw dev",
    "test": "rw test",
    "lint:check": "prettier -c .",
    "lint:write": "prettier -w .",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "2.2.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "luxon": "^3.0.1",
    "superjson": "^1.9.1"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
